#include "/home/paul/.config/colors"

*background: #050d1b
*foreground: #e6e6e6
*cursorColor: #e6e6e6

*color0: #050d1b
*color1: #c548a5
*color2: #af75cb
*color3: #00FF66
*color4: #ffb000
*color5: #dc1010
*color6: #0abdc6
*color7: #e6e6e6
*color8: #000d24
*color9: #e60068
*color10: #8d38a3
*color11: #33ff00
*color12: #e3f728
*color13: #e70d00
*color14: #0cbee4
*color15: #c5c8c6

URxvt.font: xft:Source Code Pro:size=10.5
URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt*internalBorder: 5
URxvt.geometry: 90x30
URxvt.background: #050d1b

rofi.color-enabled: true
rofi.color-window: COLOR_BACKGROUND, COLOR_TEXT_WARN, COLOR_TEXT_WARN
rofi.color-normal: COLOR_BACKGROUND, COLOR_TEXT_WARN, COLOR_BACKGROUND, COLOR_BACKGROUND, COLOR_WHITE
rofi.color-active: COLOR_BACKGROUND, COLOR_TEXT_WARN, COLOR_BACKGROUND, COLOR_BACKGROUND, COLOR_WHITE
rofi.color-urgent: COLOR_BACKGROUND, COLOR_TEXT_WARN, COLOR_BACKGROUND, COLOR_BACKGROUND, COLOR_WHITE

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro Semibold 10.5
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.dpi: 220

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

Xft.dpi:220
Xft.autohint:0
Xft.lcdfilter: lcddefault
Xft.hintstyle:hintfull
Xft.hinting:1
Xft.antialias:1
Xft.rgba:rgb
